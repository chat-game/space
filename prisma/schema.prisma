generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id        String   @id
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  twitchId  String
  userName String
  x   Int @default(0)
  y Int @default(0)
}
